{
  "name": "inkrockit-dashboard",
  "private": true,
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=inkrockit-api\" \"npm run dev --workspace=inkrockit-web\"",
    "dev:web": "npm run dev --workspace=inkrockit-web",
    "dev:api": "npm run dev --workspace=inkrockit-api",
    "build": "npm run build --workspaces",
    "lint": "npm run lint --workspaces",
    "test": "npm run test --workspaces",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
}
